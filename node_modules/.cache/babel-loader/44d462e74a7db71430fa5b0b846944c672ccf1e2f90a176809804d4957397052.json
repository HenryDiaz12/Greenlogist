{"ast":null,"code":"export default {\n  name: 'ProducerView',\n  data() {\n    return {\n      editing: false,\n      originalProducer: {\n        name: 'Juan Perez',\n        description: 'Productor de frutas orgánicas en Cusco',\n        location: 'Cusco, Perú',\n        phone: '+51 987 875 908',\n        email: 'juanperez@greenlogist.com'\n      },\n      producer: {}\n    };\n  },\n  created() {\n    this.resetProducerData();\n  },\n  methods: {\n    startEditing() {\n      this.editing = true;\n    },\n    saveChanges() {\n      console.log('Datos guardados:', this.producer);\n      this.originalProducer = {\n        ...this.producer\n      };\n      this.editing = false;\n      alert('Perfil actualizado correctamente');\n    },\n    cancelEditing() {\n      this.resetProducerData();\n      this.editing = false;\n    },\n    resetProducerData() {\n      this.producer = {\n        ...this.originalProducer\n      };\n    }\n  }\n};","map":{"version":3,"names":["name","data","editing","originalProducer","description","location","phone","email","producer","created","resetProducerData","methods","startEditing","saveChanges","console","log","alert","cancelEditing"],"sources":["C:\\Users\\Daniel\\Documents\\UPC\\GreenlogistApps\\src\\views\\perfil.vue"],"sourcesContent":["<template>\n  <div class=\"producer-container\">\n    <div class=\"profile-header\">\n      <div class=\"profile-image-container\">\n        <img \n          src=\"@/assets/producer-avatar.png\" \n          alt=\"Foto de Juan Perez\" \n          class=\"profile-image\"\n        >\n      </div>\n      \n      <h1 v-if=\"!editing\">{{ producer.name }}</h1>\n      <input \n        v-else\n        v-model=\"producer.name\" \n        class=\"edit-input\"\n      >\n      \n      <p class=\"producer-title\">Productor Orgánico Certificado</p>\n    </div>\n    \n    <div class=\"producer-content\">\n      <div class=\"profile-details\">\n        <div>\n          <p v-if=\"!editing\"><strong>Descripción:</strong> {{ producer.description }}</p>\n          <textarea\n            v-else\n            v-model=\"producer.description\"\n            class=\"edit-textarea\"\n          ></textarea>\n        </div>\n        \n        <div>\n          <p v-if=\"!editing\"><strong>Ubicación:</strong> {{ producer.location }}</p>\n          <input\n            v-else\n            v-model=\"producer.location\"\n            class=\"edit-input\"\n          >\n        </div>\n        \n        <div>\n          <p v-if=\"!editing\"><strong>Teléfono:</strong> {{ producer.phone }}</p>\n          <input\n            v-else\n            v-model=\"producer.phone\"\n            class=\"edit-input\"\n            type=\"tel\"\n          >\n        </div>\n        \n        <div>\n          <p v-if=\"!editing\"><strong>Email:</strong> {{ producer.email }}</p>\n          <input\n            v-else\n            v-model=\"producer.email\"\n            class=\"edit-input\"\n            type=\"email\"\n          >\n        </div>\n      </div>\n      \n      <div class=\"producer-buttons\">\n        <template v-if=\"!editing\">\n          <button @click=\"startEditing\" class=\"producer-button\">Editar Perfil</button>\n          <router-link to=\"/\" class=\"producer-button\">Volver al Panel</router-link>\n        </template>\n        \n        <template v-else>\n          <button @click=\"saveChanges\" class=\"producer-button save-button\">Guardar Cambios</button>\n          <button @click=\"cancelEditing\" class=\"producer-button cancel-button\">Cancelar</button>\n        </template>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'ProducerView',\n  data() {\n    return {\n      editing: false,\n      originalProducer: {\n        name: 'Juan Perez',\n        description: 'Productor de frutas orgánicas en Cusco',\n        location: 'Cusco, Perú',\n        phone: '+51 987 875 908',\n        email: 'juanperez@greenlogist.com'\n      },\n      producer: {}\n    }\n  },\n  created() {\n    this.resetProducerData();\n  },\n  methods: {\n    startEditing() {\n      this.editing = true;\n    },\n    saveChanges() {\n      console.log('Datos guardados:', this.producer);\n      this.originalProducer = {...this.producer};\n      this.editing = false;\n      \n      alert('Perfil actualizado correctamente');\n    },\n    cancelEditing() {\n      this.resetProducerData();\n      this.editing = false;\n    },\n    resetProducerData() {\n      this.producer = {...this.originalProducer};\n    }\n  }\n}\n</script>\n\n<style scoped>\n\n.producer-container {\n  max-width: 800px;\n  margin: 0 auto;\n  padding: 20px;\n}\n\n.profile-header {\n  text-align: center;\n  margin-bottom: 20px;\n}\n\n.profile-image-container {\n  width: 150px;\n  height: 150px;\n  margin: 0 auto 15px;\n  border-radius: 50%;\n  overflow: hidden;\n  border: 5px solid #4CAF50;\n  box-shadow: 0 3px 10px rgba(0,0,0,0.2);\n}\n\n.profile-image {\n  width: 100%;\n  height: 100%;\n  object-fit: cover;\n}\n\n.producer-title {\n  color: #4CAF50;\n  font-weight: bold;\n  margin-top: -10px;\n  font-style: italic;\n}\n\n.producer-content {\n  background-color: white;\n  padding: 25px;\n  border-radius: 10px;\n  box-shadow: 0 2px 15px rgba(0,0,0,0.1);\n}\n\n.profile-details {\n  margin-bottom: 20px;\n}\n\n.detail-item {\n  display: flex;\n  align-items: center;\n  margin-bottom: 12px;\n  padding-bottom: 12px;\n  border-bottom: 1px solid #eee;\n}\n\n.detail-icon {\n  margin-right: 15px;\n  font-size: 20px;\n}\n\n.detail-item p {\n  margin: 0;\n}\n\n.producer-buttons {\n  display: flex;\n  justify-content: space-between;\n  margin-top: 25px;\n  gap: 15px;\n}\n\n.producer-button {\n  background-color: #4CAF50;\n  color: white;\n  border: none;\n  padding: 12px 15px;\n  border-radius: 6px;\n  cursor: pointer;\n  font-size: 16px;\n  text-align: center;\n  text-decoration: none;\n  flex: 1;\n  transition: all 0.3s ease;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n}\n\n.producer-button:hover {\n  background-color: #3d8b40;\n  transform: translateY(-2px);\n  box-shadow: 0 4px 8px rgba(0,0,0,0.1);\n}\n\n\n.edit-input, .edit-textarea {\n  flex-grow: 1;\n  padding: 8px 12px;\n  border: 1px solid #ddd;\n  border-radius: 4px;\n  font-size: 16px;\n}\n\n.edit-textarea {\n  min-height: 80px;\n  resize: vertical;\n}\n\n.save-button {\n  background-color: #2196F3;\n}\n\n.save-button:hover {\n  background-color: #0b7dda;\n}\n\n.cancel-button {\n  background-color: #f44336;\n}\n\n.cancel-button:hover {\n  background-color: #da190b;\n}\n</style>\n"],"mappings":"AA8EA,eAAe;EACbA,IAAI,EAAE,cAAc;EACpBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,KAAK;MACdC,gBAAgB,EAAE;QAChBH,IAAI,EAAE,YAAY;QAClBI,WAAW,EAAE,wCAAwC;QACrDC,QAAQ,EAAE,aAAa;QACvBC,KAAK,EAAE,iBAAiB;QACxBC,KAAK,EAAE;MACT,CAAC;MACDC,QAAQ,EAAE,CAAC;IACb;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,iBAAiB,CAAC,CAAC;EAC1B,CAAC;EACDC,OAAO,EAAE;IACPC,YAAYA,CAAA,EAAG;MACb,IAAI,CAACV,OAAM,GAAI,IAAI;IACrB,CAAC;IACDW,WAAWA,CAAA,EAAG;MACZC,OAAO,CAACC,GAAG,CAAC,kBAAkB,EAAE,IAAI,CAACP,QAAQ,CAAC;MAC9C,IAAI,CAACL,gBAAe,GAAI;QAAC,GAAG,IAAI,CAACK;MAAQ,CAAC;MAC1C,IAAI,CAACN,OAAM,GAAI,KAAK;MAEpBc,KAAK,CAAC,kCAAkC,CAAC;IAC3C,CAAC;IACDC,aAAaA,CAAA,EAAG;MACd,IAAI,CAACP,iBAAiB,CAAC,CAAC;MACxB,IAAI,CAACR,OAAM,GAAI,KAAK;IACtB,CAAC;IACDQ,iBAAiBA,CAAA,EAAG;MAClB,IAAI,CAACF,QAAO,GAAI;QAAC,GAAG,IAAI,CAACL;MAAgB,CAAC;IAC5C;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}